{
  "name": "auto-snoo",
  "version": "1.0.11",
  "description": "A Generic, customisable reddit bot generator",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "bump": "npm version patch -m 'Bumping package version to %s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jetbooster/auto-snoo.git"
  },
  "keywords": [
    "reddit",
    "bot",
    "custom"
  ],
  "author": "jetbooster",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jetbooster/auto-snoo/issues"
  },
  "homepage": "https://github.com/jetbooster/auto-snoo#readme",
  "dependencies": {
    "snoostorm": "^1.0.2",
    "snoowrap": "^1.16.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.4",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "html",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "testMatch": [
      "**/*.test.js"
    ]
  }
}
